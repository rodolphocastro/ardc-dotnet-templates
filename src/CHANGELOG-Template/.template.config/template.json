{
    "$schema": "http://json.schemastore.org/template",
    "author": "Rodolpho Alves",
    "classifications": [
        "Common",
        "Changelog",
        "keep a changelog"
    ],
    "identity": "ARDC.Commons.Changelog",
    "name": "Changelog",
    "shortName": "changelog",
    "description": "A template for a changelog file based on keep a changelog",
    "tags": {
        "type": "item"
    },
    "symbols": {
        "projectName": {
            "type": "parameter",
            "datatype": "text",
            "description": "Your projects name",
            "isRequired": true,
            "replaces": "MyProject"
        }
    }
}